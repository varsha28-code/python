class Solution(object):
    def sumFourDivisors(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        ans = 0
        for num in nums:
            count = 0
            div_sum = 0
            for i in range(1, num + 1):
                if num % i == 0:
                    count += 1
                    div_sum += i
            if count == 4:
                ans += div_sum
        return ans

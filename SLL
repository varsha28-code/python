class Node:
    def __init__(self,val):
        self.data=val
        self.next=None
def createLinkedList(arr):
    head=None
    for val in arr:
        if(head==None):
            head=Node(val)
            temp=head
        else:
            temp.next=Node(val)
            temp=temp.next
    return head          
arr=list(map(int,input().split()))             
print(createLinkedList(arr))          


Output:
1 2 3 4 
<__main__.Node object at 0x000001A3A89EB0E0>

class Node:
    def __init__(self,val):
        self.data=val
        self.next=None
def createSCLL(arr):
    head=None
    for val in arr:
        if(head==None):
            head=Node(val)
            temp=head
        else:
            temp.next=Node(val)
            temp=temp.next
    temp.next=head
arr=list(map(int,input().split()))
print(createSCLL(arr))


Output:
1 2 3 4
None
